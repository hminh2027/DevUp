dress' className={styles.input} value={form.address} type=\"text\" spellCheck=\"false\" name=\"address\" placeholder=\"Address...\" />\r\n                        </div>\r\n                        <div className={styles.textarea_wrapper}>\r\n                            <label htmlFor=\"bio\">Bios:</label>\r\n                            <TextareaAutosize minRows='2' maxRows='4' value={form.bio} id='bio' name='bio' spellCheck='false' onChange={inputChangeHandler} className={styles.textarea} placeholder='Tell people something about you...' />\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n            <div className={styles.footer}>\r\n                <div onClick={closeModal} className={`${styles.cancel} ${styles.btn}`}>Cancel</div>\r\n                <div onClick={submitHandler} className={`${styles.submit} ${styles.btn}`}>Submit</div>\r\n            </div>\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default EditProfileModal","import React, { useState } from 'react'\r\nimport { useDispatch } from 'react-redux'\r\nimport ReactDOM from 'react-dom'\r\n\r\nimport { useMediaQuery } from 'react-responsive'\r\n\r\nimport Modal from '../util/Modal'\r\nimport EditProfileModal from '../modal/EditProfileModal'\r\n\r\nimport styles from './EditBtn.module.css'\r\n\r\nconst EditBtn = () => {\r\n    const dispatch = useDispatch()\r\n\r\n    const [isEdit, setIsEdit] = useState(false);\r\n\r\n\r\n    const showModal = () => {\r\n        dispatch({type: 'MODAL' , payload: true});\r\n        setIsEdit(true);\r\n    }\r\n\r\n    const isDesktop = useMediaQuery({ query: '(min-width: 1224px)' })\r\n\r\n    return (\r\n        <>\r\n            {isEdit && ReactDOM.createPortal(\r\n            <Modal setIsEdit={setIsEdit} width={isDesktop ? '50%' : '100%'} height={isDesktop ? 'auto' : '100%'} >\r\n                <EditProfileModal setIsEdit={setIsEdit} />\r\n            </Modal>\r\n            , portal)}\r\n\r\n           